<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dashboard.api-server</string>
    <key>ProgramArguments</key>
    <array>
        <!-- UPDATE THIS PATH to your node installation -->
        <string>/usr/local/bin/node</string>
        <string>api-server.js</string>
    </array>
    <!-- UPDATE THIS PATH to your dashboard directory -->
    <key>WorkingDirectory</key>
    <string>/path/to/mac-dashboard</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/path/to/mac-dashboard/logs/api-server.log</string>
    <key>StandardErrorPath</key>
    <string>/path/to/mac-dashboard/logs/api-server.err</string>
</dict>
</plist>
